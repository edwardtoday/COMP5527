	Journal = {MEDICAL CARE},
	Month = apr,
	Number = {{4, VA}},
	Pages = {{AS45--AS53}},
	Rating = {0},
	Title = {{Use of a cost accounting system to evaluate costs of a VA special program}},
	Url = {http://dx.doi.org/10.1097/00005650-199904002-00007%7D},
	Volume = {{37}},
	Year = {1999},
